	        <RadioButton
	          android:id="@+id/radio_case3"
	          android:textColor="@color/white"	          
	          android:text="case3 37 "/>
	        <RadioButton
	          android:id="@+id/radio_case4"
	          android:textColor="@color/white"
	          android:text="case4 33 "/>
	        <RadioButton
	          android:id="@+id/radio_case5"
	          android:textColor="@color/white"
	          android:text="case5 32 "/>
	        <RadioButton
	          android:id="@+id/radio_case6"
	          android:textColor="@color/white"
	          android:text="case6 34 "/>
	        <RadioButton
	          android:id="@+id/radio_case7"
	          android:textColor="@color/white"
	          android:text="case7 34 "/>
	        <RadioButton
	          android:id="@+id/radio_case8"
	          android:textColor="@color/white"
	          android:text="case8 33 "/>
	        <RadioButton
	          android:id="@+id/radio_case9"
	          android:textColor="@color/white"
	          android:text="case9 31 "/>
	        <RadioButton
	          android:id="@+id/radio_case10"
	          android:textColor="@color/white"
	          android:text="case10 35 "/>
	        	        	        	        	        
	        	        
	        </RadioGroup> 
	    
	</LinearLayout>
</FrameLayout>
	
